<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE hibernate-mapping
   PUBLIC "-//Hibernate/Hibernate Configuration DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 <hibernate-mapping>
   <class name="br.com.medical.model.Historico"
       dynamic-update="true"
       dynamic-insert="true"
       select-before-update="true"
       lazy="true"
       table="HISTORICO">
       <id name="codigo" column="COD" type="java.lang.Integer">
           <generator class="identity"/>
       </id>
       <property name="doenca" column="DOENCA" type="java.lang.String" length="40"/>
   </class>
 </hibernate-mapping>
